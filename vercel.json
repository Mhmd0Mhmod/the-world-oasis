{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "redirects": [
    {
      "source": "/old-route",
      "destination": "/new-route",
      "permanent": true
    },
    {
      "source": "/another-old-route",
      "destination": "/another-new-route",
      "permanent": false
    }
  ]
}
